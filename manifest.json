{
  "name": "What to Watch Tonight",
  "version": "1.0.0",
  "description": "Suggestion For Watching Movies",
  "manifest_version": 3,
  "author": "Ari23yan",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "What to Watch Tonight"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
